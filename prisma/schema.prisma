// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native"]
  // output        = "../src/generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = "file:./main.db"
}

model PageViews {
  pageId String @id
  count  Int    @default(1)
}
